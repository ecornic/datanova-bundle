{
  "name": "florianajir/datanova-bundle",
  "type": "symfony-bundle",
  "description": "LaPoste DataNOVA OpenData API client and proxy",
  "license": "MIT",
  "authors": [
    {
      "name": "Florian Ajir",
      "email": "florianajir@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "symfony/config": "^3.4 || ^4.4 || ^5.0",
    "symfony/console": "^3.4 || ^4.4 || ^5.0",
    "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
    "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
    "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7 || ^8",
    "symfony/yaml": "^3.4 || ^4.4 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Fmaj\\LaposteDatanovaBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fmaj\\LaposteDatanovaBundle\\Tests\\": "test"
    }
  }
